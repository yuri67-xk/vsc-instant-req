{
  "name": "instant-req",
  "displayName": "Instant Req",
  "description": "Claude Codeの要件定義から実装まで一貫したプロンプトを即座に生成",
  "version": "1.1.1",
  "publisher": "yxk-dev",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "instantReq",
          "title": "Instant Req",
          "icon": "./instant-plan.svg"
        }
      ]
    },
    "views": {
      "instantReq": [
        {
          "type": "webview",
          "id": "instantReq.sidePanel",
          "name": "Instant Req"
        }
      ]
    },
    "configuration": {
      "title": "Instant Req",
      "properties": {
        "instantReq.agents.specWriters": {
          "type": "array",
          "default": [
            {
              "id": "@agent-general-purpose",
              "name": "General Purpose Agent"
            }
          ],
          "description": "仕様書作成用エージェントのリスト"
        },
        "instantReq.agents.implementers": {
          "type": "array",
          "default": [
            {
              "id": "@agent-general-purpose",
              "name": "General Purpose Agent"
            }
          ],
          "description": "実装用エージェントのリスト"
        },
        "instantReq.agents.issueExplorers": {
          "type": "array",
          "default": [
            {
              "id": "@agent-explore",
              "name": "Issue Explorer"
            }
          ],
          "description": "課題探索用エージェントのリスト"
        },
        "instantReq.agents.systemArchitects": {
          "type": "array",
          "default": [
            {
              "id": "@agent-system-architect",
              "name": "System Architect"
            }
          ],
          "description": "システムアーキテクト用エージェントのリスト"
        },
        "instantReq.agents.seniorEngineers": {
          "type": "array",
          "default": [
            {
              "id": "@agent-senior-engineer",
              "name": "Senior Engineer"
            }
          ],
          "description": "シニアエンジニア用エージェントのリスト"
        },
        "instantReq.agents.codeReviewers": {
          "type": "array",
          "default": [
            {
              "id": "@agent-code-review-pro",
              "name": "Code Review Pro"
            }
          ],
          "description": "コードレビュー用エージェントのリスト"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run compile:extension && npm run compile:webview",
    "compile:extension": "tsc -p ./",
    "compile:webview": "node build-webview.js",
    "watch": "npm run watch:extension & npm run watch:webview",
    "watch:extension": "tsc -watch -p ./",
    "watch:webview": "node build-webview.js --watch",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "esbuild": "^0.27.0",
    "eslint": "^8.x",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yuri67-xk/vsc-instant-req"
  },
  "keywords": [
    "claude",
    "claude-code",
    "prompt",
    "generator",
    "ai",
    "subagents",
    "plan-mode"
  ],
  "license": "MIT",
  "dependencies": {
    "@vscode/codicons": "^0.0.41"
  }
}
